{
	"trailingComma": "all",
	"useTabs": true,
	"semi": true,
	"printWidth": 80,
	"arrowParens": "always",
	"importOrderSeparation": true,
	"importOrder": [
		"^next.*",
		"^react$",
		"^react.*",
		"^@src/.*",
		"^@models/.*",
		"^@utils/.*",
		"^@components/.*",
		".*scss$",
		".*css$",
		"^@svg/.*"
	]
}
